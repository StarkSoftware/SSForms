<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ef_ProgressWheel">
        <attr name="ef_matProg_progressIndeterminate" format="boolean" />
        <attr name="ef_matProg_barColor" format="color" />
        <attr name="ef_matProg_rimColor" format="color" />
        <attr name="ef_matProg_rimWidth" format="dimension" />
        <attr name="ef_matProg_spinSpeed" format="float" />
        <attr name="ef_matProg_barSpinCycleTime" format="integer" />
        <attr name="ef_matProg_circleRadius" format="dimension" />
        <attr name="ef_matProg_fillRadius" format="boolean" />
        <attr name="ef_matProg_barWidth" format="dimension" />
        <attr name="ef_matProg_linearProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DateSwitcher">
        <attr name="buttonTextColor" format="color"/>
        <attr name="buttonBackgroundColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="textBackgroundColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="StarkSpinner">
        <!-- View attributes -->
        <attr name="RevealViewBackgroundColor" format="color" />
        <attr name="StartSearchTintColor" format="color" />
        <attr name="SearchViewBackgroundColor" format="color" />
        <attr name="SearchViewTextColor" format="color" />
        <attr name="DoneSearchTintColor" format="color" />
        <attr name="AnimDuration" format="integer"/>
        <attr name="ShowBorders" format="boolean"/>
        <attr name="BordersSize" format="dimension"/>
        <attr name="BoarderColor" format="color"/>
        <attr name="SpinnerExpandHeight" format="dimension"/>
        <attr name="KeepLastSearch" format="boolean"/>
        <attr name="RevealEmptyText" format="string"/>
        <attr name="SearchHintText" format="string"/>
        <attr name="NoItemsFoundText" format="string"/>
        <attr name="ItemsDivider" format="reference"/>
        <attr name="DividerHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SignaturePad">
        <attr name="penMinWidth" format="dimension" />
        <attr name="penMaxWidth" format="dimension" />
        <attr name="penColor" format="color" />
        <attr name="velocityFilterWeight" format="float" />
        <attr name="clearOnDoubleClick" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SegmentedButton">

        <attr name="sb_rippleColor" format="color"/>

        <attr name="sb_drawable" format="reference"/>
        <attr name="sb_drawablePadding" format="dimension"/>
        <attr name="sb_drawableTint" format="color"/>
        <attr name="sb_drawableTint_onSelection" format="color"/>
        <attr name="sb_drawableWidth" format="dimension"/>
        <attr name="sb_drawableHeight" format="dimension"/>
        <attr name="sb_drawableKeepAspectRatio" format="boolean"/>
        <attr name="sb_drawableGravity" format="integer">
            <flag name="left" value="0"/>
            <flag name="top" value="1"/>
            <flag name="right" value="2"/>
            <flag name="bottom" value="3"/>
        </attr>

        <attr name="sb_text" format="string"/>
        <attr name="sb_textColor" format="color"/>
        <attr name="sb_textColor_onSelection" format="color"/>
        <attr name="sb_textSize" format="dimension"/>
        <attr name="sb_textTypeface" format="integer">
            <flag name="monospace" value="0"/>
            <flag name="normal" value="1"/>
            <flag name="sans" value="2"/>
            <flag name="serif" value="3"/>
        </attr>
        <attr name="sb_textTypefacePath" format="string"/>
        <attr name="sb_textFillSpace" format="boolean"/>
        <attr name="sb_textStyle" format="integer">
            <flag name="normal" value="0"/>
            <flag name="bold" value="1"/>
            <flag name="italic" value="2"/>
        </attr>
        <attr name="sb_textGravity" format="integer">
            <flag name="left" value="0"/>
            <flag name="center" value="1"/>
            <flag name="right" value="2"/>
        </attr>

        <attr name="android:layout_width"/>
        <attr name="android:layout_height"/>
        <attr name="android:layout_weight"/>

    </declare-styleable>

    <declare-styleable name="SegmentedGroup">
        <attr name="sc_corner_radius" format="dimension" />
        <attr name="sc_border_width" format="dimension" />
        <attr name="sc_tint_color" format="color" />
        <attr name="sc_checked_text_color" format="color" />
        <attr name="sc_unchecked_tint_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="AwesomeRadioButton">
        <attr name="awesome_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="SegmentedButtonGroup">
        <attr name="sbg_selectorBackgroundDrawable" format="reference"/>
        <attr name="sbg_dividerBackgroundDrawable" format="reference"/>
        <attr name="sbg_backgroundDrawable" format="reference"/>

        <attr name="sbg_borderSize" format="dimension"/>
        <attr name="sbg_borderColor" format="color"/>

        <attr name="sbg_ripple" format="boolean"/>
        <attr name="sbg_rippleColor" format="color"/>

        <attr name="sbg_selectorColor" format="color"/>

        <attr name="sbg_dividerSize" format="dimension"/>
        <attr name="sbg_dividerPadding" format="dimension"/>
        <attr name="sbg_dividerColor" format="color"/>
        <attr name="sbg_dividerRadius" format="dimension"/>

        <attr name="sbg_position" format="integer"/>
        <attr name="sbg_radius" format="dimension"/>
        <attr name="sbg_backgroundColor" format="color"/>

        <attr name="sbg_enabled" format="boolean"/>
        <attr name="android:clickable"/>
        <attr name="sbg_draggable" format="boolean"/>

        <attr name="sbg_animateSelectorDuration" format="integer"/>
        <attr name="sbg_animateSelector" format="integer">
            <flag name="fastOutSlowIn" value="0"/>
            <flag name="bounce" value="1"/>
            <flag name="linear" value="2"/>
            <flag name="decelerate" value="3"/>
            <flag name="cycle" value="4"/>
            <flag name="anticipate" value="5"/>
            <flag name="accelerateDecelerate" value="6"/>
            <flag name="accelerate" value="7"/>
            <flag name="anticipateOvershoot" value="8"/>
            <flag name="fastOutLinearIn" value="9"/>
            <flag name="linearOutSlowIn" value="10"/>
            <flag name="overshoot" value="11"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="RatingBarAttributes">
        <attr name="numStars" format="integer" />
        <attr name="rating" format="float" />
        <attr name="starPadding" format="integer" />
        <attr name="drawableEmpty" format="reference" />
        <attr name="drawableFilled" format="reference" />
        <attr name="touchable" format="boolean" />
        <attr name="clearRatingEnabled" format="boolean" />
    </declare-styleable>

</resources>
